{
  "name": "openapi-client-axios-ts",
  "description": "JavaScript client library for consuming OpenAPI-enabled APIs with axios. Types included.",
  "version": "3.9.10",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "client",
    "axios",
    "frontend",
    "browser",
    "mock",
    "typescript"
  ],
  "homepage": "https://github.com/meabed/openapi-client-axios-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meabed/openapi-client-axios-ts.git"
  },
  "bugs": {
    "url": "https://github.com/meabed/openapi-client-axios-ts/issues"
  },

  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!scripts",
    "!node_modules",
    "!src",
    "!*.config.js",
    "!typegen"
  ],
  "peerDependencies": {
    "axios": "^0.21.1"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.7",
    "bath-es5": "^3.0.3",
    "lodash": "^4.17.21",
    "openapi-types": "^7.2.3",
    "query-string": "^6.14.1"
  },
  "devDependencies": {
    "@anttiviljami/dtsgenerator": "^2.5.2",
    "@types/jest": "^26.0.20",
    "@types/json-schema": "^7.0.7",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.33",
    "@types/yargs": "^16.0.0",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "indent-string": "^4.0.0",
    "jest": "^26.6.3",
    "json-schema": "^0.3.0",
    "json-schema-deref-sync": "^0.14.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.3",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.2.3",
    "yargs": "^16.2.0"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "prepare": "npm run build",
    "test": "NODE_ENV=test jest"
  }
}
